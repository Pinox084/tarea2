package claseexpendedor;


public abstract class Moneda {
    public Moneda(){}
    public int getValor(){
        return 0;
    }
}
class Moneda1000 extends Moneda{
    public Moneda1000(){
    }
    @Override
    public int getValor(){
        return super.getValor()+1000;
    }
    @Override
    public String toString(){
       return this + "1000";
    }
}
class Moneda500 extends Moneda{
    public Moneda500(){
    }
    public int valor(){
        return super.getValor()+500;
    }
    @Override
    public String toString(){
       return this + "500";
    }
}
class Moneda100 extends Moneda{
    public Moneda100(){
    }
    public int valor(){
        return super.getValor()+100;
    }
    @Override
    public String toString(){
       return this + "100";
    }
}
